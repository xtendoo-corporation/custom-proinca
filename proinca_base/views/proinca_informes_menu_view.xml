<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_proinca_seguimiento_tree" model="ir.ui.view">
            <field name="name">informes.seguimiento.tree</field>
            <field name="model">sale.order</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="sale_order_id"/>
                    <field name="start_date"/>
                    <field name="end_date"/>
                    <field name="slide_channel_id"/>
                    <field name="student_id"/>
                    <field name="mail"/>
                    <field name="phone"/>
                    <field name="manager_id"/>
                    <field name="customer_id"/>
<!--                <field name="milestones_ids"/>-->
                    <field name="questionnaire_number"/>
                    <field name="hours"/>
                </tree>
            </field>
        </record>

        <record id="action_proinca_tracking" model="ir.actions.act_window">
            <field name="name">Seguimiento</field>
            <field name="res_model">sale.order</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="proinca_menu_informes"
                  name="Informes"
                  sequence="20"/>

        <menuitem id="proinca_menu_tracking"
                  name="Seguimiento"
                  parent="proinca_menu_informes"
                  sequence="20"
                  action="action_proinca_tracking"/>
</odoo>
