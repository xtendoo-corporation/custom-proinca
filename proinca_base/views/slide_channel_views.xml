<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="slide_channel_view_form" model="ir.ui.view">
        <field name="name">slide.channel.view.form.inherit.survey</field>
        <field name="model">slide.channel</field>
        <field name="inherit_id" ref="website_slides.view_slide_channel_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='content']" position="before">
                <page name="proinca" string="Proinca">
                    <group>
                        <field name="code"/>
                        <field name="url"/>
                        <field name="hours"/>
                        <field name="n_cuestionario"/>
                        <field name="p_hora"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
